{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Blocksi Enterprise Edition",
  "short_name": "__MSG_appShortName__",
  "description": "__MSG_appDescription__",
  "version": "3.4.3",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "background": {
    "scripts": ["scripts/background.js", "dist-nsfw/background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {},
  "content_scripts": [{
    "matches": ["http://*/*", "file://*/*", "https://*/*"],
    "js": ["jQuery/jquery.min.js", "scripts/contentscript.js", "dist-nsfw/content.js"],
    "css": ["css/raiseHand.css"],
    "run_at": "document_start",
    "all_frames": false
  }],
  "permissions": ["idle", "tabs", "http://*/*", "https://*/*", "webRequest", "webRequestBlocking", "<all_urls>", "webNavigation", "identity", "identity.email", "geolocation", "storage", "activeTab", "enterprise.deviceAttributes", "notifications", "desktopCapture", "management", "history"],
  "web_accessible_resources": ["pages/blockPage.html", "images/raise-hand.png", "images/raise-hand.svg", "images/chatBubble.svg", "images/yt-denied.png", "images/yt-shorts-block.png", "models/*"]
}